{
  "version": 2,
  "name": "demo",
  "alias": "demo.zp25.ninja",
  "public": false,
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(scripts|styles)/.+",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, s-maxage=5184000, max-age=86400"
        }
      ]
    },
    {
      "source": "/images/.+",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, s-maxage=5184000, max-age=86400"
        }
      ]
    }
  ]
}
